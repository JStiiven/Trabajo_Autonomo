
public class Tres {
	public static void main(String [] args) {
		int arreglosInt[];
		arreglosInt = new int [3];
		
		arreglosInt [0]=1;
		arreglosInt[1]=2;
		arreglosInt[2]=3;
		arreglosInt[3]=4;
		
		System.out.println(arreglosInt[0]);
		System.out.println(arreglosInt[1]);
		System.out.println(arreglosInt[2]);
		System.out.println(arreglosInt[3]);
	}
}
